{
    "num_works": 12,
    "batch_size":64,
    "heatmap_rate":0.25,
    "sigma":1.5,
    "diffusion_size":4,
    "split_name":"mini",
    "image_resize": [256,256],
    "patch_size":8,
    "model": {
        "name":"resnet",
        "image_resize": [256,256],
        "patch_size":8,
        "depth":8,
        "embed_dim":64,
        "heads":4,
        "emb_dropout":0.3,
        "dim_head":64,
        "mlp_dim":32,
        "dropout":0.3,
        "loss_func":"BCELoss",
        "in_channels": 3,
        "official_model_save":"./experiments"
    },
    "lr_strategy":{
        "warmup_epochs":10,
        "lr":1e-3,
        "min_lr":1e-6,
        "epochs":200
    },
    "train": {
        "begin_epoch": 0,
        "end_epoch": 200,
        "early_stop": 30,
        "optimizer": "adam",
        "lr": 0.001,
        "wd": 5e-4,
        "lr_step": [
            30,
            50
        ],
        "momentum": 0.0,
        "nesterov": false
    }
}